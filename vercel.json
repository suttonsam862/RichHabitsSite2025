{
  "version": 2,
  "builds": [
    {
      "src": "run-production.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/run-production.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/run-production.js" }
  ]
}